
<main class="main">


<% @most_recent_pictures.each do |picture| %>
  <figure class="caption1"><figcap class="caption"><%= picture.title %>, <%= picture.artist %></figcap></figure>
  <figure class="mainpics"><%= link_to image_tag(picture.url), "pictures/#{picture.id}" %></figure><%= Picture.created_before(1.month.ago).count %>
<% end %>
</main>

